// TC3 - Declaraciones data y operador ^
print("=== TC3: Data & ^ Instantiation ===")

data pair = {
    Pair(first:any, second:any)
}

data option = {
    None,
    Some(value:any)
}

let p1 = ^Pair(10, "ten")
let p2 = ^Pair(20, "twenty")
print("Pair 1 = " + p1)
print("Pair 2 = " + p2)

let o1 = ^Some(999)
let o2 = ^None()
print("Option 1 = " + o1)
print("Option 2 = " + o2)